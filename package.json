{
  "name": "svelte-algolia",
  "description": "Algolia server-side index updater and client-side search component for Svelte projects",
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "homepage": "https://svelte-algolia.netlify.app",
  "repository": "https://github.com/janosh/svelte-algolia",
  "license": "MIT",
  "version": "0.2.7",
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "package": "svelte-kit package",
    "serve": "yarn build && yarn preview",
    "prebuild": "cp readme.md src/docs.svx && sed -i.bak '1,/^<!-- remove above in docs -->$/d' src/docs.svx && rm src/docs.svx.bak",
    "predev": "yarn prebuild",
    "test": "ava --node-arguments='--experimental-json-modules' --timeout=2m",
    "prepublishOnly": "yarn test",
    "release": "yarn version && yarn package && yarn publish package"
  },
  "keywords": [
    "svelte",
    "algolia",
    "plugin",
    "search",
    "server-side",
    "client-side"
  ],
  "dependencies": {
    "algoliasearch": "^4.10.3"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.14",
    "@sveltejs/kit": "^1.0.0-next.141",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "ava": "^3.15.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "hastscript": "^7.0.1",
    "mdsvex": "^0.9.8",
    "prettier": "^2.3.2",
    "prettier-plugin-svelte": "^2.2.0",
    "rehype-autolink-headings": "^6.0.0",
    "rehype-slug": "^5.0.0",
    "svelte": "^3.41.0",
    "svelte-check": "^2.2.4",
    "svelte-preprocess": "^4.7.4",
    "svelte-toc": "^0.1.8",
    "svelte2tsx": "^0.4.4",
    "typescript": "^4.3.5",
    "vite": "^2.4.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
